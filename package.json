{
  "name": "psx-ng-skeleton",
  "version": "1.9.184",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=development",
    "buildProd": "ng build --configuration=production",
    "buildUat": "ng build --configuration=uat",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "version": "node -p \"require('./package.json').version\"",
    "prepare": "husky",
    "release:patch": "node ./scripts/release.js patch && npm run release:push",
    "release:minor": "node ./scripts/release.js minor && npm run release:push",
    "release:major": "node ./scripts/release.js major && npm run release:push",
    "release:patch:nopush": "node ./scripts/release.js patch",
    "release:minor:nopush": "node ./scripts/release.js minor",
    "release:major:nopush": "node ./scripts/release.js major",
    "release:push": "git push --follow-tags origin HEAD",
    "deps:check": "node ./scripts/remove-unused.mjs",
    "deps:apply": "node ./scripts/remove-unused.mjs --apply"
  },
  "standard-version": {
    "tagPrefix": "v",
    "releaseCommitMessageFormat": "chore(release): v{{currentTag}}"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.14",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.14",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@cadai/pxs-ng-core": "^1.9.25",
    "@ng-draw-flow/core": "^0.17.0",
    "@ngrx/effects": "^19.2.1",
    "@ngrx/store": "^19.2.1",
    "@ngrx/store-devtools": "^19.2.1",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "chart.js": "^4.5.0",
    "chartjs-adapter-luxon": "^1.3.1",
    "docx": "^9.5.1",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "keycloak-js": "^26.2.0",
    "luxon": "^3.7.1",
    "ngrx-store-localstorage": "^19.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/cli": "^19.2.15",
    "@angular/compiler-cli": "^19.2.0",
    "@angular/localize": "^19.2.14",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~5.1.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.2.0",
    "angular-eslint": "19.8.1",
    "conventional-commits-parser": "^6.2.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.29.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.6.2",
    "standard-version": "^9.5.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "8.34.1"
  }
}
